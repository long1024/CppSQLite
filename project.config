[project]
	description = A simple and easy-to-use cross-platform C++ wrapper for the SQLite API. Fork of the CppSQLite project, originally by Rob Groves, currently updated and maintained by NeoSmart Technologies.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/long1024
[access "refs/for/refs/*"]
	read = group user/long1024
	push = group user/long1024
	label-Code-Review = -2..+2 group user/long1024
	label-Verified = -1..+1 group user/long1024
	rebase = group user/long1024
	removeReviewer = group user/long1024
	submit = group user/long1024
[access "refs/heads/*"]
	read = group user/long1024
	create = group user/long1024
	push = +force group user/long1024
	pushMerge = group user/long1024
[access "refs/tags/*"]
	push = group user/long1024
	push = +force group user/long1024
